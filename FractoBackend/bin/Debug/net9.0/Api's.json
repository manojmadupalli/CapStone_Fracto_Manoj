{
  "info": {
    "name": "Fracto API Collection (With Auth)",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{jwtToken}}",
        "type": "string"
      }
    ]
  },
  "variable": [
    { "key": "baseUrl", "value": "http://localhost:5000/api" },
    { "key": "jwtToken", "value": "" }
  ],
  "item": [
    {
      "name": "Register User",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/auth/register", "host": ["{{baseUrl}}"], "path": ["auth", "register"] },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"username\": \"john_doe\",\n  \"fullName\": \"John Doe\",\n  \"email\": \"john@example.com\",\n  \"password\": \"password123\",\n  \"city\": \"Los Angeles\"\n}"
        }
      }
    },
    {
      "name": "Login",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/auth/login", "host": ["{{baseUrl}}"], "path": ["auth", "login"] },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"username\": \"john_doe\",\n  \"password\": \"password123\"\n}"
        }
      }
    },
    {
      "name": "Get All Users",
      "request": {
        "method": "GET",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/users", "host": ["{{baseUrl}}"], "path": ["users"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] }
      }
    },
    {
      "name": "Get User by Id",
      "request": {
        "method": "GET",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/users/1", "host": ["{{baseUrl}}"], "path": ["users", "1"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] }
      }
    },
    {
      "name": "Update User",
      "request": {
        "method": "PUT",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/users/1", "host": ["{{baseUrl}}"], "path": ["users", "1"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"fullName\": \"Johnathan Doe\",\n  \"city\": \"San Francisco\",\n  \"profileImagePath\": \"uploads/profiles/john.png\"\n}"
        }
      }
    },
    {
      "name": "Delete User",
      "request": {
        "method": "DELETE",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/users/1", "host": ["{{baseUrl}}"], "path": ["users", "1"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] }
      }
    },
    {
      "name": "Get All Specializations",
      "request": {
        "method": "GET",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/specializations", "host": ["{{baseUrl}}"], "path": ["specializations"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] }
      }
    },
    {
      "name": "Add Specialization",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/specializations", "host": ["{{baseUrl}}"], "path": ["specializations"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] },
        "body": { "mode": "raw", "raw": "{ \"specializationName\": \"Orthopedics\" }" }
      }
    },
    {
      "name": "Update Specialization",
      "request": {
        "method": "PUT",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/specializations/1", "host": ["{{baseUrl}}"], "path": ["specializations", "1"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] },
        "body": { "mode": "raw", "raw": "{ \"specializationName\": \"Pediatrics\" }" }
      }
    },
    {
      "name": "Delete Specialization",
      "request": {
        "method": "DELETE",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/specializations/1", "host": ["{{baseUrl}}"], "path": ["specializations", "1"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] }
      }
    },
    {
      "name": "Get All Doctors",
      "request": {
        "method": "GET",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/doctors", "host": ["{{baseUrl}}"], "path": ["doctors"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] }
      }
    },
    {
      "name": "Get Doctor by Id",
      "request": {
        "method": "GET",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/doctors/1", "host": ["{{baseUrl}}"], "path": ["doctors", "1"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] }
      }
    },
    {
      "name": "Add Doctor",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/doctors", "host": ["{{baseUrl}}"], "path": ["doctors"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"Dr. Smith\",\n  \"city\": \"New York\",\n  \"specializationId\": 1,\n  \"rating\": 4.5,\n  \"profileImagePath\": \"uploads/profiles/default.png\"\n}"
        }
      }
    },
    {
      "name": "Update Doctor",
      "request": {
        "method": "PUT",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/doctors/1", "host": ["{{baseUrl}}"], "path": ["doctors", "1"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] },
        "body": { "mode": "raw", "raw": "{\n  \"name\": \"Dr. John Smith\",\n  \"city\": \"Chicago\",\n  \"rating\": 4.8\n}" }
      }
    },
    {
      "name": "Delete Doctor",
      "request": {
        "method": "DELETE",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/doctors/1", "host": ["{{baseUrl}}"], "path": ["doctors", "1"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] }
      }
    },
    {
      "name": "Get All Appointments",
      "request": {
        "method": "GET",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/appointments", "host": ["{{baseUrl}}"], "path": ["appointments"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] }
      }
    },
    {
      "name": "Get Appointment by Id",
      "request": {
        "method": "GET",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/appointments/1", "host": ["{{baseUrl}}"], "path": ["appointments", "1"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] }
      }
    },
    {
      "name": "Book Appointment",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/appointments", "host": ["{{baseUrl}}"], "path": ["appointments"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"userId\": 2,\n  \"doctorId\": 1,\n  \"appointmentDate\": \"2025-09-05T10:00:00\",\n  \"status\": \"Booked\"\n}"
        }
      }
    },
    {
      "name": "Update Appointment",
      "request": {
        "method": "PUT",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/appointments/1", "host": ["{{baseUrl}}"], "path": ["appointments", "1"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] },
        "body": { "mode": "raw", "raw": "{ \"status\": \"Cancelled\" }" }
      }
    },
    {
      "name": "Delete Appointment",
      "request": {
        "method": "DELETE",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/appointments/1", "host": ["{{baseUrl}}"], "path": ["appointments", "1"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] }
      }
    },
    {
      "name": "Upload File",
      "request": {
        "method": "POST",
        "header": [{ "key": "Content-Type", "value": "application/json" }],
        "url": { "raw": "{{baseUrl}}/files/upload", "host": ["{{baseUrl}}"], "path": ["files", "upload"] },
        "auth": { "type": "bearer", "bearer": [{ "key": "token", "value": "{{jwtToken}}", "type": "string" }] }
      }
    }
  ]
}
